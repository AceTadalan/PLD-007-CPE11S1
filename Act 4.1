#include <iostream>
using namespace std;

int main() {
	 
	 float physics, chemistry, biology, math, computer, total, percentage;
	 char grade;
	 
	 cout << "Enter your grades for Physics: ";
	 cin >> physics; 
	 
	 cout << "Enter your grades for Chemistry: ";
	 cin >> chemistry; 
	 
	 cout << "Enter your grades for Biology: ";
	 cin >> biology; 
	 
	 cout << "Enter your grades for Math: ";
	 cin >> math; 
	 
	 cout << "Enter your grades for Computer: ";
	 cin >> computer;
	 
	 total = physics + chemistry + biology + math + computer;
	 percentage = (total / 500) * 100;
	 
	 switch (static_cast <int>(percentage) / 10) {
	 	case 10: 
	 	case 9:
	 		grade = 'A';
	 		break;
	 	case 8:
	 		grade = 'B';
	 		break;
	 	case 7:
	 		grade = 'C';
	 		break;
	 	case 6:
	 		grade = 'D';
	 		break;
	 	case 4:
	 	case 5:
	 		grade = 'E';
	 		break;
	 		default:
	 			grade = 'F';
	 			
	 }
	 cout << "" << endl;	
	 cout << "Physics: " << physics << endl;
	 cout << "Chemistry: " << chemistry << endl;
	 cout << "Biology: " << biology << endl;
	 cout << "Math: " << math << endl;
	 cout << "Computer: " << computer << endl;
	 cout << "" << endl;
	 cout << "Average: " << percentage << "%" << endl;
	 cout << "Grade Level: " << grade << endl;
	 
	
	
	return 0;
	
}
